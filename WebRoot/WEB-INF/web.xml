<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>CheckAccount</servlet-name>
    <servlet-class>servlet.CheckAccount</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Food1</servlet-name>
    <servlet-class>servlet.Food1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FoodEditServlet</servlet-name>
    <servlet-class>servlet.FoodEditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckAccount</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Food1</servlet-name>
    <url-pattern>/Food</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FoodEditServlet</servlet-name>
    <url-pattern>/FoodEdit</url-pattern>
  </servlet-mapping>
</web-app>